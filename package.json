 {
   "name": "monetized-blog-app",
   "version": "1.0.0",
   "description": "Full-stack blog application optimized for SEO and monetization through AdSense and affiliate marketing",
   "main": "index.js",
   "type": "module",
   "scripts": {
     "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
     "server:dev": "cd backend && npm run dev",
     "client:dev": "cd frontend && npm run dev",
     "server:start": "cd backend && npm start",
     "client:build": "cd frontend && npm run build",
     "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
     "test": "echo \"Error: no test specified\" && exit 1"
   },
   "keywords": ["blog", "seo", "monetization", "adsense", "affiliate-marketing", "react", "node.js"],
   "author": "Your Name",
   "license": "MIT",
   "devDependencies": {
     "concurrently": "^8.2.2"
   }
 }